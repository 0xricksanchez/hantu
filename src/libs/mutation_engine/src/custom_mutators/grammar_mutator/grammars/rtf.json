  "<style_prop>": [["<style_prop_def>", "<style_prop>"], []],
  "<style_prop_def>": [
    ["\\sbasedon", "<style_name>"],
    ["\\snext", "<style_name>"],
    ["\\slink", "<style_name>"],
    ["\\s", "<control_word_body>", "<style_value>"]
  ],
  "<style_value>": [["<string>"]],
  "<info>": [["{\\info", "<info_type>", "<info_props>", "}"]],
