    ["<control_symbol>", "<group_body>"],
    ["<object>"],
    ["<footnote>"],
    ["<endnote>"]
  ],
  "<group_body>": [["<content>"]],
  "<control_word>": [["\\", "<alpha>", "<control_word_body>"]],
