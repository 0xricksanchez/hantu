    ["$((1+1))"],
    ["$((var*2))"],
    ["$((var1/var2))"],
    ["$((var1%var2))"]
  ],
  "<compound_command>": [
    ["<if_statement>"],
    ["<while_loop>"],
    ["<until_loop>"],
    ["<for_loop>"],
    ["<case_statement>"],
    ["<function_declaration>"]
  ],
  "<if_statement>": [
    ["if [ <expression> ]; then\\n", "<commands>", "fi"],
    [
      "if [ <expression> ]; then\\n",
      "<commands>",
      "else\\n",
      "<commands>",
      "fi"
    ],
    [
      "if [ <expression> ]; then\\n",
      "<commands>",
      "elif [ <expression> ]; then\\n",
      "<commands>",
      "fi"
    ]
  ],
  "<expression>": [
    ["-e <filename>"],
    ["-z <string>"],
    ["<string> == <string>"],
    ["<number> -eq <number>"],
    ["<variable> -eq <variable>"],
    ["<arithmetic_expression>"]
  ],
  "<while_loop>": [["while [ <expression> ]; do\\n", "<commands>", "done"]],
  "<until_loop>": [["until [ <expression> ]; do\\n", "<commands>", "done"]],
  "<for_loop>": [
    ["for file in <filename_list>; do\\n", "<commands>", "done"],
    ["for i in <number_list>; do\\n", "<commands>", "done"],
    ["for ((i=0; i< <number>; i++)); do\\n", "<commands>", "done"]
  ],
  "<filename_list>": [["*"], ["*.txt"], ["file{1..3}.txt"]],
  "<number_list>": [["{1..10}"], ["{1..10..2}"]],
  "<case_statement>": [
    ["case <variable> in\\n", "<case_pattern_list>", "esac"]
  ],
  "<case_pattern_list>": [
    ["<case_pattern>"],
    ["<case_pattern>", "<case_pattern_list>"]
  ],
  "<case_pattern>": [["<pattern>)", "<commands>;;"]],
  "<pattern>": [["word"], ["word|word2"], ["word*)"]],
  "<function_declaration>": [
    ["function <function_name>() {\\n", "<commands>", "}\\n"],
    ["<function_name>() {\\n", "<commands>", "}\\n"]
  ],
  "<function_name>": [["my_function"], ["custom_function"], ["helper"]]
